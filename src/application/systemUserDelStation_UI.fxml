<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UI_Call_Handler">
   <children>
      <Pane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: indigo;">
         <children>
            <Button layoutX="200.0" layoutY="34.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="400.0" style="-fx-background-color: maroon;" text="Delete a Station" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Century Gothic Bold" size="24.0" />
               </font>
            </Button>
            <Button layoutX="102.0" layoutY="295.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: white;" text="Station ID" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Century Gothic Bold" size="18.0" />
               </font>
            </Button>
            <TextField fx:id="sID" layoutX="367.0" layoutY="295.0" prefHeight="40.0" prefWidth="300.0" />
            <Button layoutX="283.0" layoutY="492.0" mnemonicParsing="false" onAction="#systemUserDeletedStation" prefHeight="53.0" prefWidth="234.0" style="-fx-background-color: maroon;" text="Submit" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Century Gothic Bold" size="24.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
